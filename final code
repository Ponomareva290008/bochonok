# -*- coding: utf-8 -*-
import random
N = int(input('Введите количество бочонков в мешке:'))
list = []
for i in range(1, N+1):   #заполнение списка
    list.append(i)
for i in range(len(list)):     
    a = random.choice(list)    #выбирается случайное число из списка
    list.remove(a)             #удаление этого числа, чтоб оно не повторялось
    print(a)                   #вывод
